    {"field": "evidence_type", "label": "Evidence", "type": "string"},
    {"field": "original_reference_title", "label": "Title", "type": "string"},
    {"field": "review.summary", "label": "Summary", "type": "string"},
    {"field": "review.action", "label": "Action", "type": "string"},
    {"field": "review.reason", "label": "Reason", "type": "string"},
    {"field": "review.supporting_text", "label": "Ref Text", "type": "string"},
    {"field": "review.supporting_reference_ids", "label": "RefIDs", "type": "curie"},
    {"field": "review.proposed_replacement_terms", "label": "Replacements", "type": "string"}
